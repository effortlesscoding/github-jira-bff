    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "runtimeVersion": "20.0.0",
            "request": "launch",
            "name": "Launch Current JS File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}